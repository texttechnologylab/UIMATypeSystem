<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">

    <name>TypeSystemReadability</name>

    <description>Readability of the whole Text</description>

    <version>1.0</version>

    <vendor/>

    <imports>

        <import name="desc.type.TextTechnologyAnnotation"/>

        <import name="desc.type.TypeSystemModelMeta"/>


    </imports>
    <types>
        <typeDescription>
            <name>org.texttechnologylab.annotation.Readability</name>
            <description>
                Readability Output
            </description>
            <supertypeName>uima.cas.AnnotationBase</supertypeName>
            <features>

                <featureDescription>

                    <name>TextReadabilities</name>

                    <description>Set of Text Readability with key and Value</description>

                    <rangeTypeName>uima.cas.FSArray</rangeTypeName>

                    <elementType>org.texttechnologylab.annotation.AnnotationComment</elementType>

                </featureDescription>

                <featureDescription>

                    <name>model</name>

                    <description>Which Model or Process was used to Compute the TextReadability</description>

                    <rangeTypeName>org.texttechnologylab.annotation.model.MetaData</rangeTypeName>

                </featureDescription>
            </features>
        </typeDescription>
        <typeDescription>
            <name>org.texttechnologylab.annotation.ReadabilityAdvance</name>
            <description>
                Readability Output
            </description>
            <supertypeName>org.texttechnologylab.annotation.Readability</supertypeName>
            <features>
                <featureDescription>

                    <name>GroupName</name>

                    <description>Group Name for the Text Readabilities</description>

                    <rangeTypeName>uima.cas.FSArray</rangeTypeName>

                    <elementType>org.texttechnologylab.annotation.AnnotationComment</elementType>

                </featureDescription>
            </features>
        </typeDescription>
    </types>
</typeSystemDescription>
